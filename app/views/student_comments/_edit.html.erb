<% form_for(@student_comment, :url=>"/student_comments/#{@student_comment.id}.js", :html=>{:multipart => true, :target=>"upload_frame", :method => :put}) do |f| %>
  <%= render :partial=>f %>
<% end %>
  

  <%= link_to_function 'cancel',{:href=> url_for(current_student)} do |page| 
    page[dom_id(@student_comment)].replace_html(:partial=>'student_comments/comment', :object=>@student_comment)
    end  %>
