<h1>New student</h1>

<% form_for(@student, :url=>district_students_url, :method=>:post) do |f| %>
  <%= f.error_messages %>

  <%= render :partial => f %>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>
<%= render :partial => 'enrollments/new_link' %> <br />

<%= link_to 'Back', district_students_path %>
