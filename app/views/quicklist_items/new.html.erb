<h1>New quicklist_item</h1>

<% form_for(@quicklist_item) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :school %><br />
    <%= f.text_field :school %>
  </p>
  <p>
    <%= f.label :district %><br />
    <%= f.text_field :district %>
  </p>
  <p>
    <%= f.label :intervention_definition %><br />
    <%= f.text_field :intervention_definition %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', quicklist_items_path %>
