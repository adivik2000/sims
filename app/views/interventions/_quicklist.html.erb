<% unless @quicklist_intervention_definitions.blank? %>
<div id ="quicklist">
  <%form_for :quicklist_item, :url => quicklist_interventions_path do |f| %>
    <%= f.label :intervention_definition_id, "Intervention Quicklist" %>
    <%= f.collection_select :intervention_definition_id, @quicklist_intervention_definitions, :id, :title ,{},:onchange=>"submit()"%>
    <%= f.submit "Pick from Quicklist" %>

  <% end %>
  </div>
<% end %>


