<p>
  <b>User:</b>
  <%=h @intervention.user.fullname %>
</p>

<p>
  <b>Student:</b>
  <%=h @intervention.student.fullname %>
</p>

<p>
  <b>Start date:</b>
  <%=h @intervention.start_date %>
</p>

<p>
  <b>End date:</b>
  <%=h @intervention.end_date %>
</p>

<p>
  <b>Intervention definition:</b>
  <%=h @intervention.intervention_definition.title %>
  <%= links_and_attachments @intervention.intervention_definition, :p %>
</p>

  <%= links_and_attachments @intervention, :p %>
<p>
  <b> Objective Definition </b>
  <%=h @intervention.objective_definition.title %>
  <%= links_and_attachments @intervention.objective_definition, :p %>
</p>
<p>
  <b>Frequency:</b>
  <%=h @intervention.frequency.title %>
</p>

<p>
  <b>Frequency multiplier:</b>
  <%=h @intervention.frequency_multiplier %>
</p>

<p>
  <b>Time length:</b>
  <%=h @intervention.time_length.title %>
</p>

<p>
  <b>Time length number:</b>
  <%=h @intervention.time_length_number %>
</p>

<%unless @intervention.active? %>
  <p>
    <b>Ended by:</b>
    <%=h @intervention.ended_by.fullname %>
  </p>

  <p>
    <b>Ended at:</b>
    <%=h @intervention.ended_at %>
  </p>

<% end %>

<%= render :partial => 'participants' %>

<%= link_to 'Edit', edit_intervention_path(@intervention) %> |
<%= link_to 'Delete', intervention_path(@intervention), :method=>:delete,:confirm=>"Are you sure you want to delete this intervention?" %> |
<% if @intervention.active? %>
  <%= link_to 'End', end_intervention_path(@intervention), :method=>:put,:confirm=>"Are you sure you want to end this intervention?" %> |
<% end %>
<%= link_to 'Back', @intervention.student %>


<%= render :partial=>"probes" %>


